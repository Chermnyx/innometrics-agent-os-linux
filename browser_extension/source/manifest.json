{
	"name": "Innometrics Extension",
	"version": "0.0.2",
	"description": "Track browser tabs for innometrics",
	"homepage_url": "https://innometrics-12856.firebaseapp.com/",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"applications": {
		"gecko": {
			"id": "innometrics-extension@innometrics-12856.firebaseapp.com",
			"strict_min_version": "67.0"
		}
	},
	"icons": {
		"128": "icon.png"
	},
	"permissions": ["storage", "tabs", "<all_urls>"],
	"background": {
		"persistent": false,
		"scripts": ["browser-polyfill.min.js", "background.js"]
	}
}
